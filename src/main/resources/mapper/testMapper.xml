<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nwu.mapper.testMapper">
    <select id="getRoleById" parameterType="string" resultType="com.nwu.entities.Role">
        select *
        from apply
        where tutor_id = #{id};
    </select>
</mapper>